{
  "name": "huns-mall",
  "author": "Dongho Lee and Hunmin Park and Hyejin Kim",
  "description": "Shopping mall client",
  "packageManager": "yarn@3.2.2",
  "scripts": {
    "typing": "typed-scss-modules \"src/**/*.module.{css,scss}\"",
    "dev": "concurrently \"yarn typing --watch\" \"next dev\"",
    "build": "yarn typing && next build",
    "start": "next start",
    "lint": "next lint --dir src",
    "lint:fix": "next lint --dir src --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.10.12",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "next": "^12.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recoil": "^0.7.5"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.3.1",
    "@types/node": "^18.7.6",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "concurrently": "^7.3.0",
    "eslint": "^8.22.0",
    "eslint-config-next": "^12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "sass": "^1.54.4",
    "typed-scss-modules": "^6.5.0",
    "typescript": "^4.7.4"
  }
}
