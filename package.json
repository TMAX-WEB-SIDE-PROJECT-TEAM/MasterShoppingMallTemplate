{
  "name": "huns-mall",
  "author": "Dongho Lee and Hunmin Park and Hyejin Kim",
  "description": "Shopping mall client",
  "packageManager": "yarn@3.3.0",
  "scripts": {
    "typing": "typed-scss-modules \"src/**/*.module.{css,scss}\"",
    "dev": "concurrently \"yarn typing --watch\" \"next dev\"",
    "build": "yarn typing && next build",
    "start": "next start",
    "lint": "next lint --dir src",
    "lint:fix": "next lint --dir src --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.10.15",
    "axios": "^1.2.0",
    "classnames": "^2.3.2",
    "next": "^13.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.3",
    "recoil": "^0.7.6",
    "remark-gfm": "^3.0.1",
    "swiper": "8.4.5"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.5.1",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "concurrently": "^7.6.0",
    "eslint": "^8.28.0",
    "eslint-config-next": "^13.0.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.0",
    "sass": "^1.56.1",
    "typed-scss-modules": "^7.0.1",
    "typescript": "^4.9.3"
  }
}
